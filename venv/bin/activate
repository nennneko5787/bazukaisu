# This file must be used with "source bin/activate" *from a-Shell*
# you cannot run it directly

deactivate
setenv _OLD_VIRTUAL_PATH $PATH
setenv _OLD_VIRTUAL_PS1  $PS1
setenv PYTHONPYCACHEPREFIX $__VENV_DIR__/__pycache__
setenv PYTHONUSERBASE $__VENV_DIR__
setenv PATH $__VENV_DIR__/bin:$PATH
setenv VIRTUAL_ENV $__VENV_DIR__
setenv PS1 (venv):$PS1
